{
    "manifest_version": 2,
    "name": "Cambridge Dictionary",
    "description": "Searches highlighted text on Cambridge Dictionary",
    "version": "2.0",
    "icons": {
        "48": "icons/dictionary.png",
        "96": "icons/dictionary.png"
    },
    "permissions": ["contextMenus"],
    "background": {
        "scripts":["background.js"]
    },
    "sidebar_action": {
        "default_panel": "/index.html",
        "open_at_install": false
    }
}